{
  "name": "@ko-platform/image-editor",
  "version": "1.4.0",
  "description": "",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.tsx",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "lib/",
    "src/"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sv:patch": "standard-version --release-as patch",
    "sv:minor": "standard-version --release-as minor",
    "sv:major": "standard-version --release-as major",
    "prepare": "bob build",
    "release": "release-it"
  },
  "keywords": [
    "react",
    "react-native",
    "image",
    "image editor",
    "image cropping",
    "expo"
  ],
  "author": "adscud",
  "license": "MIT",
  "homepage": "https://github.com/ko-platform/ko-image-editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/ko-platform/ko-image-editor"
  },
  "dependencies": {
    "@release-it/conventional-changelog": "^3.3.0",
    "expo-image-picker": "^11.0.3",
    "modal-enhanced-react-native-web": "^0.2.0",
    "no-scroll": "^2.1.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-responsive": "^9.0.0-beta.5",
    "recoil": "^0.0.10"
  },
  "peerDependencies": {
    "@expo/vector-icons": "*",
    "expo-asset": "*",
    "expo-file-system": "*",
    "expo-gl-cpp": "*",
    "expo-image-manipulator": "*",
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@expo/vector-icons": "^12.0.5",
    "@types/lodash": "^4.14.157",
    "@types/react": "~16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-native": "~0.63.2",
    "babel-preset-expo": "~8.1.0",
    "expo-asset": "^8.1.7",
    "expo-file-system": "^9.2.0",
    "expo-gl-cpp": "^9.1.2",
    "expo-image-manipulator": "^9.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-builder-bob": "^0.18.2",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-web": "~0.13.12",
    "release-it": "^14.11.6",
    "standard-version": "^9.2.0",
    "typescript": "^3.9.3"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
