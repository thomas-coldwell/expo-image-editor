{
  "name": "@ko-platform/image-editor",
  "version": "1.5.3",
  "description": "",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.ts",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "lib/",
    "src/"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sv:patch": "standard-version --release-as patch",
    "sv:minor": "standard-version --release-as minor",
    "sv:major": "standard-version --release-as major",
    "prepare": "bob build",
    "release": "release-it"
  },
  "keywords": [
    "react",
    "react-native",
    "image",
    "image editor",
    "image cropping",
    "expo"
  ],
  "author": "adscud",
  "license": "MIT",
  "homepage": "https://github.com/ko-platform/ko-image-editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ko-platform/ko-image-editor"
  },
  "dependencies": {
    "expo-image-manipulator": "^10.1.2",
    "no-scroll": "^2.1.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@expo/vector-icons": "^12.0.5",
    "@release-it/conventional-changelog": "^3.3.0",
    "@types/react": "~16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-native": "~0.63.2",
    "babel-preset-expo": "~8.1.0",
    "expo-image-picker": "^11.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "0.66.3",
    "react-native-builder-bob": "^0.18.2",
    "release-it": "^14.11.6",
    "standard-version": "^9.2.0",
    "typescript": "^3.9.3"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "publishConfig": {
    "@ko-platform:registry": "https://npm.pkg.github.com"
  }
}
